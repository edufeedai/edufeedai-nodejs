{
    "compilerOptions": {
      "module": "ES6",
      "target": "ES6",
      "moduleResolution": "Bundler",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  